      "source": [
        "h=eval(input(\"Enter Diamond's height: \"))\n",
        "for i in range(h):\n",
        "  if(i<h/2):\n",
        "    print(\" \"*(int((h/2)-i)),\"#\"*((i*2)+1))\n",
        "  else:\n",
        "    print(\" \"*(int(i-(h/2)+1)),\"#\"*((h-i)*2-1))"
